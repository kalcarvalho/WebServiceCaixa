<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://caixa.gov.br/sibar/consulta_cobranca_bancaria/boleto" 
			xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
			xmlns:sibar_base="http://caixa.gov.br/sibar" 
			xmlns:consultacobrancabancaria="http://caixa.gov.br/sibar/consulta_cobranca_bancaria/boleto">

	<xsd:import schemaLocation="../BAR_MSG_BARRAMENTO_LIB/MensagensBarramento.xsd"
		namespace="http://caixa.gov.br/sibar"/>

    <xsd:element name="SERVICO_ENTRADA"
        type="consultacobrancabancaria:SERVICO_ENTRADA_NEGOCIAL_TYPE"/>

    <xsd:complexType name="SERVICO_ENTRADA_NEGOCIAL_TYPE">
        <xsd:complexContent>
            <xsd:extension base="sibar_base:SERVICO_ENTRADA_TYPE">
                <xsd:sequence>
                    <xsd:element name="DADOS" type="consultacobrancabancaria:DADOS_ENTRADA_TYPE"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="DADOS_ENTRADA_TYPE">
        <xsd:sequence>
        	<xsd:choice>
        		<xsd:element name="CONSULTA_SOLICITACAO_GRUPO_PAGADOR"
        			type="consultacobrancabancaria:CONSULTA_SOLICITACAO_GRUPO_PAGADOR_INPUT_TYPE" />
        		<xsd:element name="CONSULTA_SOLICITACAO_PRE_IMPRESSO"
        			type="consultacobrancabancaria:CONSULTA_SOLICITACAO_PRE_IMPRESSO_INPUT_TYPE" />
        		<xsd:element name="CONSULTA_SOLICITACOES_GRUPO_PAGADOR"
        			type="consultacobrancabancaria:CONSULTA_SOLICITACOES_GRUPO_PAGADOR_INPUT_TYPE" />
        		<xsd:element name="CONSULTA_SOLICITACOES_PRE_IMPRESSO"
        			type="consultacobrancabancaria:CONSULTA_SOLICITACOES_PRE_IMPRESSO_INPUT_TYPE" />
        		<xsd:element name="IMPRIME_BOLETO"
        			type="consultacobrancabancaria:IMPRIME_BOLETO_INPUT_TYPE" />
                <xsd:element name="CONSULTA_BOLETO" type="consultacobrancabancaria:consulta_boleto_entrada_Type" minOccurs="1" maxOccurs="1"></xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

	<xsd:element name="SERVICO_SAIDA" 
		type="consultacobrancabancaria:SERVICO_SAIDA_NEGOCIAL_TYPE"/>

	<xsd:complexType name="SERVICO_SAIDA_NEGOCIAL_TYPE">
		<xsd:complexContent>
			<xsd:extension base="sibar_base:SERVICO_SAIDA_TYPE">
				<xsd:sequence>
					<xsd:element name="DADOS" type="consultacobrancabancaria:DADOS_SAIDA_TYPE" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DADOS_SAIDA_TYPE">
		<xsd:complexContent>
			<xsd:extension base="sibar_base:DADOS_SAIDA_TYPE">
				<xsd:sequence>
					<xsd:choice minOccurs="0">
						<xsd:element
							name="CONSULTA_SOLICITACAO_GRUPO_PAGADOR"
							type="consultacobrancabancaria:CONSULTA_SOLICITACAO_GRUPO_PAGADOR_TYPE" />
						<xsd:element
							name="CONSULTA_SOLICITACAO_PRE_IMPRESSO"
							type="consultacobrancabancaria:CONSULTA_SOLICITACAO_PRE_IMPRESSO_TYPE" />
						<xsd:element
							name="CONSULTA_SOLICITACOES_GRUPO_PAGADOR"
							type="consultacobrancabancaria:CONSULTA_SOLICITACOES_GRUPO_PAGADOR_TYPE" />
						<xsd:element
							name="CONSULTA_SOLICITACOES_PRE_IMPRESSO"
							type="consultacobrancabancaria:CONSULTA_SOLICITACOES_PRE_IMPRESSO_TYPE" />
						<xsd:element name="IMPRIME_BOLETO"
							type="consultacobrancabancaria:IMPRIME_BOLETO_TYPE" />
						<xsd:element name="CONSULTA_BOLETO"
							type="consultacobrancabancaria:consulta_boleto_saida_Type" minOccurs="1" maxOccurs="1">
						</xsd:element>
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


<!-- CONSULTA SOLICITACAO DE GRUPO DE PAGADORES -->

    <xsd:complexType name="CONSULTA_SOLICITACAO_GRUPO_PAGADOR_INPUT_TYPE">
        <xsd:sequence>
			<xsd:element name="CODIGO_BENEFICIARIO">
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CODIGO_GRUPO">
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DATA_SOLICITACAO" type="xsd:date"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

	<xsd:complexType name="CONSULTA_SOLICITACAO_GRUPO_PAGADOR_TYPE">
		<xsd:sequence>
			<xsd:element name="DATA_VENCIMENTO" type="xsd:date"></xsd:element>
			<xsd:element name="VALOR_TITULO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="9999999999999.99"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PRAZO_PROTESTO_DEVOLUCAO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="9999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DESCRICOES" type="consultacobrancabancaria:DESCRICOES_TYPE" />
			<xsd:element name="COMPLEMENTARES" type="consultacobrancabancaria:COMPLEMENTARES_TYPE" minOccurs="0"/>
			<xsd:element name="SITUACAO">
        		<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
        	</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="DESCRICOES_TYPE">
		<xsd:sequence>
			<xsd:element name="REGISTRO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="OBTER_ENVIO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="30"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ENVIO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="OBTER_VALOR" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="30"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PROTESTO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ESPECIE" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ACEITE" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ENDOSSO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="COMPLEMENTARES_TYPE">
		<xsd:sequence>
			<xsd:element name="DESCONTOS" minOccurs="0" type="consultacobrancabancaria:DESCONTOS_TYPE" />
			<xsd:element name="MULTA" minOccurs="0" type="consultacobrancabancaria:DESCONTO_MULTA_TYPE" />
			<xsd:element name="JUROS" minOccurs="0" type="consultacobrancabancaria:JUROS_TYPE" />
			<xsd:element name="IMPOSTOS" minOccurs="0" type="consultacobrancabancaria:IMPOSTOS_TYPE" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="DESCONTOS_TYPE">
		<xsd:sequence>
			<xsd:element name="DESCONTO" type="consultacobrancabancaria:DESCONTO_MULTA_TYPE" maxOccurs="3"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="DESCONTO_MULTA_TYPE">
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="PERCENTUAL">
					<xsd:simpleType>
						<xsd:restriction base="xsd:decimal">
							<xsd:minInclusive value="0"></xsd:minInclusive>
							<xsd:maxInclusive value="999.99"></xsd:maxInclusive>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="VALOR"> 
					<xsd:simpleType>
						<xsd:restriction base="xsd:decimal">
							<xsd:minInclusive value="0"></xsd:minInclusive>
							<xsd:maxInclusive value="9999999999999.99"></xsd:maxInclusive>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:choice>
			<xsd:choice>
				<xsd:element name="PRAZO">
					<xsd:simpleType>
						<xsd:restriction base="xsd:short">
							<xsd:minInclusive value="0"></xsd:minInclusive>
							<xsd:maxInclusive value="999"></xsd:maxInclusive>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="DATA" type="xsd:date"></xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="JUROS_TYPE">
		<xsd:sequence>
			<xsd:element name="TIPO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="9"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="VALOR" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999.99"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="IMPOSTOS_TYPE">
		<xsd:sequence>
			<xsd:element name="VALOR_IOF" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="9999999999999.99"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>


<!-- CONSULTA SOLICITACAO DE PRE-IMPRESSOS -->

    <xsd:complexType name="CONSULTA_SOLICITACAO_PRE_IMPRESSO_INPUT_TYPE">
        <xsd:sequence>
			<xsd:element name="CODIGO_BENEFICIARIO">
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DATA_SOLICITACAO" type="xsd:date"></xsd:element>
			<xsd:element name="TIPO_BLOQUETO">
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="9"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
        </xsd:sequence>
    </xsd:complexType>

	<xsd:complexType name="CONSULTA_SOLICITACAO_PRE_IMPRESSO_TYPE">
		<xsd:sequence>
			<xsd:element name="NOSSO_NUMERO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:long">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999999999999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="TIPO_ENTREGA" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="QUANTIDADE_BLOQUETO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="TIPO_COBRANCA" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SITUACAO">
        		<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
        	</xsd:element>
		</xsd:sequence>
	</xsd:complexType>


<!-- CONSULTA LISTA DE SOLICITACOES DE BOLETOS PRE-IMPRESSOS -->

    <xsd:complexType name="CONSULTA_SOLICITACOES_PRE_IMPRESSO_INPUT_TYPE">
        <xsd:sequence>
        	<xsd:element name="CODIGO_BENEFICIARIO">
        		<xsd:simpleType>
        			<xsd:restriction base="xsd:int">
        				<xsd:minInclusive value="0"></xsd:minInclusive>
        				<xsd:maxInclusive value="999999"></xsd:maxInclusive>
        			</xsd:restriction>
        		</xsd:simpleType>
        	</xsd:element>
        	<xsd:element name="TIPO_BOLETO" minOccurs="0">
        		<xsd:simpleType>
        			<xsd:restriction base="xsd:string">
        				<xsd:maxLength value="1"></xsd:maxLength>
        			</xsd:restriction>
        		</xsd:simpleType>
        	</xsd:element>
        	<xsd:element name="DATA_INICIO" type="xsd:date" minOccurs="0"></xsd:element>
        	<xsd:element name="DATA_FIM" type="xsd:date" minOccurs="0"></xsd:element>
        	<xsd:element name="FLAG_SITUACAO">
        		<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
        	</xsd:element>
        	<xsd:element name="PAGINA">
        		<xsd:simpleType>
        			<xsd:restriction base="xsd:int">
        				<xsd:minInclusive value="0"></xsd:minInclusive>
        				<xsd:maxInclusive value="999999"></xsd:maxInclusive>
        			</xsd:restriction>
        		</xsd:simpleType>
        	</xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CONSULTA_SOLICITACOES_PRE_IMPRESSO_TYPE">
        <xsd:sequence>
			<xsd:element name="QUANTIDADE_PAGINAS" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
            <xsd:element name="SOLICITACOES" type="consultacobrancabancaria:SOLICITACOES_PRE_IMPRESSO_TYPE" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>

	<xsd:complexType name="SOLICITACOES_PRE_IMPRESSO_TYPE">
		<xsd:sequence>
			<xsd:element name="SOLICITACAO" type="consultacobrancabancaria:SOLICITACAO_PRE_IMPRESSO_TYPE" minOccurs="0" maxOccurs="20"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="SOLICITACAO_PRE_IMPRESSO_TYPE">
		<xsd:sequence>
			<xsd:element name="DATA_SOLICITACAO"  type="xsd:date"></xsd:element>
			<xsd:element name="QUANTIDADE_BOLETOS" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:long">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="NOSSO_NUMERO_INICIAL" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:long">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999999999999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DESCRICAO_TIPO_ENTREGA" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DESCRICAO_TIPO_BOLETO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
        	<xsd:element name="SITUACAO">
        		<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
        	</xsd:element>
		</xsd:sequence>
	</xsd:complexType>


<!-- CONSULTA LISTA DE SOLICITACOES DE BOLETOS PARA GRUPO DE PAGADORES -->

    <xsd:complexType name="CONSULTA_SOLICITACOES_GRUPO_PAGADOR_INPUT_TYPE">
        <xsd:sequence>
			<xsd:element name="CODIGO_BENEFICIARIO">
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DATA_INICIO" type="xsd:date" minOccurs="0"></xsd:element>
			<xsd:element name="DATA_FIM" type="xsd:date" minOccurs="0"></xsd:element>
        	<xsd:element name="FLAG_SITUACAO">
        		<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
        	</xsd:element>
			<xsd:element name="PAGINA">
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
        </xsd:sequence>
    </xsd:complexType>

	<xsd:complexType name="CONSULTA_SOLICITACOES_GRUPO_PAGADOR_TYPE">
		<xsd:sequence>
			<xsd:element name="QUANTIDADE_PAGINAS" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SOLICITACOES" type="consultacobrancabancaria:SOLICITACOES_GRUPO_PAGADOR_TYPE" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

	<xsd:complexType name="SOLICITACOES_GRUPO_PAGADOR_TYPE">
		<xsd:sequence>
			<xsd:element name="SOLICITACAO" type="consultacobrancabancaria:SOLICITACAO_GRUPO_PAGADOR_TYPE" minOccurs="0" maxOccurs="20"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="SOLICITACAO_GRUPO_PAGADOR_TYPE">
		<xsd:sequence>
			<xsd:element name="DATA_SOLICITACAO"  type="xsd:date"></xsd:element>
			<xsd:element name="CODIGO_GRUPO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="999"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="NOME_GRUPO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DESCRICAO_ESPECIE" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DATA_VENCIMENTO"  type="xsd:date"></xsd:element>
			<xsd:element name="VALOR_TITULO" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"></xsd:minInclusive>
						<xsd:maxInclusive value="9999999999999.99"></xsd:maxInclusive>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
        	<xsd:element name="SITUACAO">
        		<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
        	</xsd:element>
		</xsd:sequence>
	</xsd:complexType>


<!-- IMPRIME BOLETO -->

    <xsd:complexType name="IMPRIME_BOLETO_INPUT_TYPE">
